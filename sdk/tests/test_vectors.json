  "replay_protection": {
    "description": "Tests for counter/nonce replay protection",
    "tests": [
      {
        "description": "Sequential counters should be accepted",
        "counters": [1, 2, 3, 4, 5],
        "expected": [true, true, true, true, true]
      },
      {
        "description": "Duplicate counter should be rejected",
        "counters": [1, 2, 3, 2, 4],
        "expected": [true, true, true, false, true]
      },
      {
        "description": "Out of order within window should be accepted",
        "window_size": 64,
        "counters": [1, 2, 3, 5, 4],
        "expected": [true, true, true, true, true]
      },
      {
        "description": "Counter outside window should be rejected",
        "window_size": 64,
        "counters": [1, 2, 67, 1],
        "expected": [true, true, true, false]
      }
    ]
  },

